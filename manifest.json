{

  "manifest_version": 2,
  "name": "Roll20 Character Sheet IO",
  "version": "1.0",

  "description": "Provides import and export to file options for roll20 character sheets.",

  "icons": {
      "48": "icon.svg"
  },


  "content_scripts": [
    {
      "matches": ["*://app.roll20.net/editor/*"],
      "js": ["roll20-io-bootstrap.js"]
    }
  ],

  "web_accessible_resources": ["roll20-io-payload.js", "FileSaver.js"]
}
